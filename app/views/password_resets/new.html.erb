<h1>パスワードの再設定</h1>

<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
      <%= f.label :email, 'メールアドレス' %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.submit "送信", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
